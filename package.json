{
  "name": "@marcalexiei/prettier-config",
  "description": "Custom prettier config",
  "version": "1.1.1",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 20"
  },
  "keywords": [
    "format",
    "prettier",
    "prettier-config"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/marcalexiei/prettier-config"
  },
  "author": "Marco Pasqualetti @marcalexiei",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "packageManager": "pnpm@10.14.0",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "24.5.0",
      "onFail": "download"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "10.14"
    }
  },
  "scripts": {
    "build": "rolldown -c rolldown.config.ts && tsc --project tsconfig.types.json",
    "typecheck": "tsc --noEmit",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "node --test ./*.spec.js",
    "release": "pnpm run build && changeset publish"
  },
  "peerDependencies": {
    "prettier": "^2.8.0 || ^3.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.4",
    "@types/node": "22.15.19",
    "prettier": "3.5.3",
    "rolldown": "1.0.0-beta.9",
    "typescript": "5.8.3"
  }
}
